{
	"name": "transformcsv",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "transfor_csv",
						"type": "DatasetReference"
					},
					"name": "sourceCSV"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "sinkfatfilecsv",
						"type": "DatasetReference"
					},
					"name": "sinkFatfile"
				},
				{
					"dataset": {
						"referenceName": "sinkdataflowcsv",
						"type": "DatasetReference"
					},
					"name": "sinkotherfattype"
				}
			],
			"transformations": [
				{
					"name": "selectcolumn"
				},
				{
					"name": "filterRows"
				},
				{
					"name": "splitfiles"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Item_Identifier as string,",
				"          Item_Weight as double,",
				"          Item_Fat_Content as string,",
				"          Item_Visibility as double,",
				"          Item_Type as string,",
				"          Item_MRP as double,",
				"          Outlet_Identifier as string,",
				"          Outlet_Establishment_Year as short,",
				"          Outlet_Size as string,",
				"          Outlet_Location_Type as string,",
				"          Outlet_Type as string,",
				"          Item_Outlet_Sales as double",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceCSV",
				"sourceCSV select(mapColumn(",
				"          Item_Identifier,",
				"          Item_Weight,",
				"          Item_Fat_Content,",
				"          Item_Visibility,",
				"          Item_Type,",
				"          Item_MRP,",
				"          Outlet_Type,",
				"          Item_Outlet_Sales",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectcolumn",
				"selectcolumn filter(Outlet_Type != 'Grocery Store') ~> filterRows",
				"filterRows split(Item_Fat_Content == 'Low Fat',",
				"     Item_Fat_Content != 'Low Fat',",
				"     disjoint: false) ~> splitfiles@(FatTypelowfat, FatTypeothers)",
				"splitfiles@FatTypelowfat sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkFatfile",
				"splitfiles@FatTypeothers sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkotherfattype"
			]
		}
	}
}